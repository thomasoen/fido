	<script language="javascript" type="text/javascript">
		if (typeof B2 == "undefined") { var B2 = {}; }
		
		var DEFAULT_SEARCH_TERM = "Search for business name or category";
		var DEFAULT_SEARCH_LOCATION = "Location (e.g. 10010 or New York, NY)";
		var skipLocUpdate = false;
		$(document).ready(function(){
			B2.Core.init();
			B2.GeoLocation.HTML5.init();
			B2.Rec.Auth.init();
			
			try{ B2.Common.Advertise.init();}
			catch(err){ B2.Logger.log('ad unit script not loaded'); }
		});
		
    var _gaq = _gaq || [];
	  
    _gaq.push(['_setAccount', 'UA-15170101-1']);
    _gaq.push(['_setDomainName', '.bundle.com']);
    _gaq.push(['_trackPageview']);
    

    (function() {
        var ga = document.createElement('script');     ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:'   == document.location.protocol ? 'https://ssl'   : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

    
  
		

	</script>
