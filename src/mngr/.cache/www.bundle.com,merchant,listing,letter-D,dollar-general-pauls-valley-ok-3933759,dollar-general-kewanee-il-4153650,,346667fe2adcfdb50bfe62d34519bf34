<div class="feature-box">
    <div id="adZone-206325" class="advert-mod"></div>
</div>
 











<div class="feature-box">
  <h2>Our Free Newsletter</h2>
  <div class="feature-box-section">
    <p>Get more great insights delivered to you Inbox. Sign up for Bundle's FREE Newsletter!</p>
    <form id="subscribe-form" action="/services/email-subscribe/" method="POST">
      <div class="input-wrapper one-line-form">
        <input type="text" id="id-email" name="email"/>
      </div>
      <div class="input-wrapper one-line-form">
        <input type="text" id="id-zip" name="zip"/>
        <input type="submit" id="id-submit" name="submit" value="" />
      </div>
    </form>
    <div id="subscribe-status"></div>
    <a href="/privacy-policy" class="legalese">privacy policy</a>
  </div>
</div>

<script>
  email_subscribe = {
      init: function() {
          eventName = 'Newsletter Widget Sign up'
          actionName = 'Side Rail'

          // trigger analytics event, on view:
          if (typeof B2.Merchant != "undefined") {
          	label = 'Merchant Search';
          } else if (typeof B2.Article == "undefined") {
            label = 'Content Landing'
          } else {
            label = B2.Article.slug;
          }
          B2.Tracking.trackEvent(eventName, actionName, label, true)

          // clear email subscribe input when clicked
          B2.Core.Util.buildDefaultPrefill(
              $('#id-email'),
              'Enter email address',
              'faded'
          );
          B2.Core.Util.buildDefaultPrefill(
              $('#id-zip'),
              'Enter your ZIP code',
              'faded'
          );

          $("#subscribe-form").submit(function(e) {
              B2.Tracking.trackEvent(eventName, actionName, 'Submitted', true)
              e.preventDefault();
              var email = "", zip = "";
              email = $("#id-email").attr("value");
              zip = $("#id-zip").attr("value")
              // make ajax call to fetch csrf token
              csrfToken = ""
              setCsrfToken = function(value) {
                csrfToken = value['csrf_token'];

                // post request to /email-subscribe/ handler
                $.ajax({
                    url: "/services/email-subscribe/",
                    type: "POST",
                    data: {"source":"right-rail", "email":email, "zip":zip, "csrfmiddlewaretoken":csrfToken},
                    success: function(response) {
                        if (response.indexOf('Thanks') == 0) {
                            $("#id-email").attr("disabled", "disabled");
                            $("#id-zip").attr("disabled", "disabled");
                            $("#id-submit").attr("disabled", "disabled");
                            $("#id-submit").addClass("opacitated");
                            B2.Tracking.trackEvent(eventName, actionName, 'ResponseSuccess', true)
                        } else {
                            B2.Tracking.trackEvent(eventName, actionName, 'ResponseUserError', true)
                        }
                        // update the front end if user successfully subscribes
                        $("#subscribe-status").html(response);
                        $("#subscribe-status").hide().fadeIn("fast");
                    },
                    error: function (response) {
                        B2.Tracking.trackEvent(eventName, actionName, 'ResponseServerError', true)
                        $("#subscribe-status").html("We had a server error, and couldn't subscribe you. But please, try again later, thanks!");
                        $("#subscribe-status").hide().fadeIn("fast");
                    }
                });   
              }
              $.ajax({
                url: "/services/fetch-csrf/",
                type: "POST",
                async: false,
                success: setCsrfToken,
                error: function (response) {
                  B2.Tracking.trackEvent(eventName, actionName, 'ResponseServerCsrfError', true)
                  $("#subscribe-status").html("We had a server error, and couldn't subscribe you. But please, try again later, thanks!");
                  $("#subscribe-status").hide().fadeIn("fast");
                }
              })
              // provide visual feedback to user
              $("#subscribe-status").html("Sending your request...");
              $("#subscribe-status").hide().fadeIn("fast");
          });
      }
  };
  $(document).ready(function(){
    // init the newsletter signup
    email_subscribe.init();
  });
</script>







